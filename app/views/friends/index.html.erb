<% if user_signed_in? %>
<h1>Friends</h1>

<div id="friends" class="jumbotron">
  <% @friends.each do |friend| %>
    <%= render friend %>
    
  <% end %>
</div>

<%= link_to "New friend", new_friend_path,class: "btn btn-secondary" %>

<% else %>

<h1>Hello World</h1>
Welcome to rails Tutorial!


<% end %>
