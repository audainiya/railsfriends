
<div id="<%= dom_id friend %>">

  <% if(friend.user==current_user) %>
  <p>
    <strong>Name:</strong>
    <%= link_to friend.first_name+" "+friend.last_name, friend %>
  </p>


  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= friend.twitter %>
  </p>

<% end %>
</div>
